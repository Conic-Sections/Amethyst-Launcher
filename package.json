{
    "name": "conic-launcher",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "main": "main.ts",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "preview": "vite preview",
        "tauri": "tauri",
        "ts:check": "tsc --noEmit",
        "lint": "eslint \"src/**\" --ignore-pattern \"src/assets\"",
        "lint:fix": "eslint \"src/**\" --ignore-pattern \"src/assets\" --fix",
        "format": "yarn prettier --write .",
        "format:check": "yarn prettier --check .",
        "clean-app-data": "rm -rf ~/.conic-debug"
    },
    "license": "GPL-3.0-only",
    "dependencies": {
        "@tauri-apps/api": ">=2.0.0-rc.0",
        "@tauri-apps/plugin-clipboard-manager": "^2.0.1",
        "@tauri-apps/plugin-dialog": "~2",
        "@tauri-apps/plugin-http": "^2.0.0-rc.0",
        "@tauri-apps/plugin-log": "~2",
        "@tauri-apps/plugin-shell": ">=2.0.0-rc.0",
        "@tauri-apps/plugin-updater": "^2.0.0-rc.0",
        "gsap": "^3.12.5",
        "jquery": "^3.6.4",
        "less": "^4.1.3",
        "less-loader": "^11.1.0",
        "pinia": "^2.2.2",
        "vue": "^3.3.4",
        "vue-i18n": "9"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.2.0",
        "@tauri-apps/cli": ">=2.0.0-rc.0",
        "@types/gsap": "^3.0.0",
        "@types/jquery": "^3.5.29",
        "@types/node": "^18.14.2",
        "@types/web": "^0.0.163",
        "@vitejs/plugin-vue": "^5.0.5",
        "@vitejs/plugin-vue-jsx": "^3.0.0",
        "@vue/eslint-config-typescript": "^11.0.2",
        "@vue/tsconfig": "^0.1.3",
        "eslint": "^8.34.0",
        "eslint-plugin-vue": "^9.9.0",
        "prettier": "^3.3.3",
        "typescript": "^5.6.2",
        "vite": "^5.3.1",
        "vite-plugin-vue-devtools": "^7.4.6",
        "vitest": "^0.29.1",
        "vue-tsc": "^2.0.22",
        "yarn": "1.22.19"
    }
